{
  "name": "single-use",
  "version": "0.1.0",
  "main": "./pages/index.tsx",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "next": "^11.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-three-fiber": "^4.0.28",
    "react-use": "^14.1.0",
    "react-youtube": "^7.12.0",
    "three": "^0.115.0"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.0",
    "@types/three": "^0.103.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "serve": "^11.1.0",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint:fix",
      "yarn prettier"
    ]
  },
  "resolutions": {
    "@typescript-eslint/parser": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dist": "yarn build && yarn export",
    "export": "next export",
    "lint": "eslint './{src/**/*.{ts,tsx},pages/*.{ts,tsx}}'",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --single-quote --trailing-comma es5 --write './{src/**/*.{ts,tsx},pages/*.{ts,tsx}}'",
    "sass": "sass ./src/styles/index.scss ./out/index.css && rm ./out/index.css.map",
    "start": "next start",
    "serve": "serve -p 8080 out",
    "serve:https": "yarn serve --ssl-cert localhost.crt --ssl-key localhost.key",
    "typecheck": "tsc --noEmit"
  }
}
